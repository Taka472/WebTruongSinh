<%
blobToImage(blob) {
    return new Promise(resolve => {
        const url = URL.createObjectURL(blob)
        let img = new Image()
        img.onload = () => {
            URL.revokeObjectURL(url)
            resolve(img)
        }
        img.src = url
        img.class = 'header';
        img.id = "title" 
    })
}
